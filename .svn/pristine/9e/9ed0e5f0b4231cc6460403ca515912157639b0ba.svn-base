				}, 2000);
			});
			H.$dropupToAppend(function(ret, err) {
				H.$openFrame('loading', 'widget://res/load11.html', null, {
					x : 0,
					y : 0
				});
				setTimeout(function() {
					H.closeFrame("loading");
