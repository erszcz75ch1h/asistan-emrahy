			.push-fixed, .refresh-fixed {
				width: 40px;
				height: 40px;
				border-radius: 50%;
				-webkit-border-radius: 50%;
				background: rgba(78,129,174,0.8);
				position: fixed;
				bottom: 10px;
				right: 10px;
				z-index: 10000;
			}
			.refresh-fixed {
				bottom: 60px;
			}
			.winu-center-all {
				display: -webkit-box;
